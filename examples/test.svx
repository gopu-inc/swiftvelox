// Test complet de SwiftVelox
fn main() {
    swget("=== TEST SWIFTVELOX ===")
    
    // Variables et calculs
    let a = 10
    let b = 20
    
    swget("a = " + a as string)
    swget("b = " + b as string)
    swget("a + b = " + (a + b) as string)
    swget("a * b = " + (a * b) as string)
    
    // Conditions
    if a > 5 {
        swget("a > 5 ✓")
    }
    
    if b > a {
        swget("b > a ✓")
    }
    
    // Chaînes
    let message = "Résultat: "
    swget(message + (a + b) as string)
    
    swget("=== FIN DU TEST ===")
}
